{
  "name": "point3en",
  "version": "1.0.0",
  "description": "About",
  "main": "main.js",
  "scripts": {
    "start": "concurrently -k \"npm run start:backend\" \"npm run start:react\" \"npm run start:electron\"",
    "start:backend": "cd backend && python app.py",
    "start:react": "cd client && react-scripts start",
    "start:electron": "wait-on tcp:3000 tcp:5000 && electron . --dev",
    "build": "cd client && react-scripts build",
    "test": "cd client && react-scripts test",
    "eject": "cd client && react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rabbit-magic-wh/Point3LearningToolEn.git"
  },
  "author": "Discover304",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rabbit-magic-wh/Point3LearningToolEn/issues"
  },
  "homepage": "https://github.com/rabbit-magic-wh/Point3LearningToolEn#readme",
  "dependencies": {
    "client": "file:client"
  },
  "devDependencies": {
    "electron": "^18.2.3",
    "electron-packager": "^15.5.1",
    "concurrently": "^7.2.2",
    "wait-on": "^6.0.1"
  }
}
